#: venv/lib/python3.8/site-packages/django/contrib/postgres/apps.py:45
msgid "PostgreSQL extensions"
msgstr "ملحقات Postgres"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:21
#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:16
#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/array.py:170
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "عضو %(nth)sم آرایه معتبر نیست:"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "آرایه های تو در تو باید هم سایز باشند"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "نگاشتی از رشته به رشته/هیچمقدار"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "مقدار \"%(key)s\" یک رشته حرفی یا null نیست."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:16
msgid "Could not load JSON data."
msgstr "امکان بارگزاری  داده های JSON  نیست."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Input must be a JSON dictionary."
msgstr "مقدار ورودی باید یک دیکشنری JSON باشد."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:33
msgid "Enter two valid values."
msgstr "دو مقدار معتبر وارد کنید"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:34
msgid "The start of the range must not exceed the end of the range."
msgstr "مقدار شروع بازه باید از پایان کوچکتر باشد"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:82
msgid "Enter two whole numbers."
msgstr "دو عدد کامل وارد کنید"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:88
msgid "Enter two numbers."
msgstr "دو عدد وارد کنید"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:94
msgid "Enter two valid date/times."
msgstr "دو تاریخ/ساعت معتبر وارد کنید"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/forms/ranges.py:100
msgid "Enter two valid dates."
msgstr "دو تاریخ معتبر وارد کنید"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:12
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"لیست شامل %(show_value)d مورد است. ولی باید حداکثر شامل %(limit_value)d مورد "
"باشد."
msgstr[1] ""
"لیست شامل %(show_value)d مورد است. ولی باید حداکثر شامل %(limit_value)d مورد "
"باشد."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:19
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"لیست شامل %(show_value)d است، نباید کمتر از %(limit_value)d را شامل شود."
msgstr[1] ""
"لیست شامل %(show_value)d است، نباید کمتر از %(limit_value)d را شامل شود."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:29
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "برخی کلیدها یافت نشدند: %(keys)s"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:30
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "برخی کلیدهای ارائه شده ناشناخته‌اند: %(keys)s"

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:70
#, python-format
msgid ""
"Ensure that this range is completely less than or equal to %(limit_value)s."
msgstr "اطمیمنان حاصل کنید که این رنج، کوچکتر یا برابر با %(limit_value)s است."

#: venv/lib/python3.8/site-packages/django/contrib/postgres/validators.py:76
#, python-format
msgid ""
"Ensure that this range is completely greater than or equal to "
"%(limit_value)s."
msgstr "اطمینان حاصل کنید که رنج، بزرگتر یا برابر با %(limit_value)s است."
